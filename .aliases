#!/bin/zsh
# Shortcuts for common git commands.
alias gs="git status";
alias gd="git diff";
alias gc="git commit";
alias ga="git add";
alias gm="git mv";
alias gr="git rm";
alias gl="git log --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit";
